<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">

    <Label fx:id="username" layoutX="9.0" layoutY="8.0" text="Welcome Anthony" AnchorPane.leftAnchor="20.0"
           AnchorPane.topAnchor="20.0">
        <font>
            <Font size="32.0"/>
        </font>
    </Label>
    <Label fx:id="role" layoutX="23.0" layoutY="55.0" text="Your role is Sales" AnchorPane.topAnchor="55.0">
        <font>
            <Font size="16.0"/>
        </font>
        <padding>
            <Insets top="10.0"/>
        </padding>
    </Label>
    <Label fx:id="currentTime" layoutX="23.0" layoutY="80.0" text="It is now 04-09-2023 12:00">
        <font>
            <Font size="16.0"/>
        </font>
        <padding>
            <Insets top="10.0"/>
        </padding>
    </Label>

</AnchorPane>
