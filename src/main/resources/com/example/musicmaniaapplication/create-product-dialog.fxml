<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="450.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="30.0" layoutY="26.0" spacing="15.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <Label fx:id="createProductErrorMessage" layoutX="115.0" layoutY="157.0" textFill="RED" />
            <TextField fx:id="productName" promptText="Name" />
            <TextField fx:id="productCategory" promptText="Category" />
            <TextField fx:id="productPrice" promptText="Price" />
            <TextField fx:id="productStock" promptText="Stock" />
            <TextArea fx:id="productDescription" prefHeight="200.0" prefWidth="200.0" promptText="Description" />
            <HBox spacing="15.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#createButtonClick" text="Create" />
                  <Button mnemonicParsing="false" onAction="#closeButtonClick" text="Create" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
