<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">

    <Label layoutX="20.0" layoutY="174.0" text="Order History" AnchorPane.leftAnchor="20.0"
           AnchorPane.topAnchor="20.0">
        <font>
            <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <TableView fx:id="orderHistoryTableView" layoutX="14.0" layoutY="47.0" prefHeight="231.0" AnchorPane.leftAnchor="20.0"
               AnchorPane.rightAnchor="20.0">
        <columns>
            <TableColumn fx:id="orderDate" minWidth="150.0" prefWidth="75.0" text="Date/Time">
            </TableColumn>
            <TableColumn fx:id="customerFullName" minWidth="250.0" prefWidth="75.0" text="Name">
            </TableColumn>
            <TableColumn fx:id="totalPrice" minWidth="100.0" prefWidth="75.0" text="Total Price">
            </TableColumn>
        </columns>
    </TableView>

</AnchorPane>
